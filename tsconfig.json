{
	"compilerOptions": {
		"allowSyntheticDefaultImports": true,
		"jsx": "react-jsx",
		"lib": [
			"dom",
			"esnext"
		],
		"moduleResolution": "node",
		"noEmit": true,
		"skipLibCheck": true,
		"downlevelIteration": true,
		"sourceMap": true,
		"experimentalDecorators": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"noImplicitReturns": true,
		"esModuleInterop": true
	},
	"types": [
		"react",
		"react-dom",
		"react-native"
	],
	"exclude": [
		"build",
		"node_modules",
		"App.js"
	],
	"compileOnSave": false,
	"extends": "expo/tsconfig.base"
}